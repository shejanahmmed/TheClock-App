<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="240dp"
    android:height="240dp"
    android:viewportWidth="240"
    android:viewportHeight="240">

    <!-- Static Capsule at 3 o'clock -->
    <!-- Center 120, 120 -->
    <!-- Position: Right side. -->
    <!-- Let's say it starts at x=150 and goes to x=220. Width=70. -->
    <!-- Height (thickness) = 50. y from 95 to 145. -->
    
    <path
        android:fillColor="#FF000000"
        android:strokeColor="#FFFFFFFF"
        android:strokeWidth="3"
        android:pathData="
            M 150,95
            L 200,95
            A 25,25 0 0 1 200,145
            L 150,145
            A 25,25 0 0 1 150,95
            Z" />
            
    <!-- 
       M 150,95 : Top-Left
       L 200,95 : Top-Right line
       A 25,25 ... : Right Arc (180 deg)
       L 150,145 : Bottom-Left line
       A 25,25 ... : Left Arc
       Z
    -->
</vector>
